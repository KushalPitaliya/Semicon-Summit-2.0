{
    "name": "semiconductor-summit-server",
    "version": "1.0.0",
    "description": "Backend server for Semiconductor Summit 2.0",
    "main": "index.js",
    "scripts": {
        "start": "node index.js",
        "dev": "nodemon index.js",
        "seed": "node seeders/seed.js",
        "seed:fresh": "node seeders/seed.js --fresh",
        "seed:users": "node seeders/seed.js --users",
        "seed:gallery": "node seeders/seed.js --gallery",
        "test": "jest --coverage --detectOpenHandles",
        "test:watch": "jest --watch",
        "test:announcements": "jest tests/announcement.test.js"
    },
    "dependencies": {
        "bcryptjs": "^3.0.3",
        "cloudinary": "^2.9.0",
        "cors": "^2.8.5",
        "dotenv": "^16.6.1",
        "express": "^4.18.2",
        "express-mongo-sanitize": "^2.2.0",
        "express-rate-limit": "^8.2.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^9.1.6",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^8.0.1",
        "pdf-parse": "^1.1.1",
        "winston": "^3.19.0"
    },
    "devDependencies": {
        "jest": "^29.7.0",
        "nodemon": "^3.0.2",
        "supertest": "^6.3.4"
    },
    "jest": {
        "testEnvironment": "node",
        "coveragePathIgnorePatterns": [
            "/node_modules/",
            "/tests/"
        ],
        "testMatch": [
            "**/tests/**/*.test.js"
        ]
    }
}
